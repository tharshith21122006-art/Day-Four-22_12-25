class StudentGrade {
    public static void main(String[] args) {
        int[] marks = {80, 75, 90, 85, 70};
        int total = 0;

        for (int m : marks)
            total += m;

        int avg = total / marks.length;

        System.out.println("Average: " + avg);

        if (avg >= 90)
            System.out.println("Grade A");
        else if (avg >= 75)
            System.out.println("Grade B");
        else
            System.out.println("Grade C");
    }
}
